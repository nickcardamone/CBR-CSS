@article{david_f_andersen_scripts_0000,
	title = {Scripts for group model building},
	volume = {13},
	abstract = {Suggests that modelers who engage in modeling with groups rely on fairly sophisticated pieces of small group process, called scripts; and describes a full multi-day modeling conference as a sequence of scripts placed to create a continuous stream of small-group activity. The authors propose that the collective process of group model building can be moved forward if these small group scripts are tried by those in the field; and discuss planning for a group model building conference, scheduling the day, particular scripts and techniques for various group model building tasks, and closing a group modeling conference. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
