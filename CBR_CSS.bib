	keywords = {Leadership, Innovation, Organizational Culture, Ireland, Action Research, Training, Models, Communities of Practice, Community Involvement, Ethnography, Feedback (Response), Foreign Countries, Human Resources, Industry, Marketing, Public Schools, Quality Control, Strategic Planning, Success, Systems Approach, Undergraduate Students},
