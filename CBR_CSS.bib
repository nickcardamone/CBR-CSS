@article{miles_participatory_2011,
	title = {Participatory model assessment of earthquake-induced landslide hazard models},
	volume = {56},
	url = {http://pitt.idm.oclc.org/login?url=https://search.proquest.com/docview/852311612?accountid=14709 http://RT4RF9QN2Y.search.serialssolutions.com?ctx_ver=Z39.88-2004&ctx_enc=info:ofi/enc:UTF-8&rfr_id=info:sid/ProQ%3Aasfaocean&rft_val_fmt=info:ofi/fmt:kev:mtx:journal&rft.genre=article&rft.jtitle=Natural+Hazards&rft.atitle=Participatory+model+assessment+of+earthquake-induced+landslide+hazard+models&rft.au=Miles%2C+Scott+B&rft.aulast=Miles&rft.aufirst=Scott&rft.date=2011-03-01&rft.volume=56&rft.issue=3&rft.spage=749&rft.isbn=&rft.btitle=&rft.title=Natural+Hazards&rft.issn=0921030X&rft_id=info:doi/10.1007%2Fs11069-010-9587-5},
	abstract = {The study described in this paper investigates the relative merits of two peer-reviewed earthquake-induced landslide models using participatory model assessment. The earthquake-induced landslide hazard models assessed are a simplified Newmark's displacement model and a recently developed knowledge-based model. Participatory model assessment involves conducting facilitated participatory processes where the model(s) are used for aiding decisions within a socio-behavioral experiment designed for collecting data to evaluate formal hypotheses about the model(s). The paper sets out the design of the participatory model assessment--a series of workshops involving experts and potential model end-users that incorporated a roleplay site selection task. Quantitative data elicited using a set of entrance and exit questionnaires were analyzed to investigate hypotheses about the models. Participants found the knowledge-based model to be significantly more complete and more informative for their roleplay task. Overall, the two models did not yield significant differences with respect to issues such as task efficiency or task outcome satisfaction. Lastly, it was found that education level and disciplinary perspectives (of those analyzed) did not significantly affect outcomes, suggesting that a wide demographic of participants can be used for participatory model assessments. Additional research is needed to assess the models in different contexts, as well as more broadly developing a set of best practices for conducting participatory model assessments of other natural hazard and risk models intended to support decision-making.[PUBLICATION ABSTRACT]},
	language = {English},
	number = {3},
	journal = {Natural Hazards},
	author = {Miles, Scott B.},
	month = mar,
	year = {2011},
	note = {Place: Dordrecht
Publisher: Springer Nature B.V.},
	keywords = {Mathematical models, Risk assessment, Earth Sciences, Earthquakes, Landslides \& mudslides},
	pages = {749--766},
}

@article{zorrilla_evaluation_2010,
	title = {Evaluation of bayesian networks as a tool for participatory water resources management: application to the upper guadiana basin in spain},
	volume = {15},
	url = {http://pitt.idm.oclc.org/login?url=https://search.proquest.com/docview/907173072?accountid=14709 http://RT4RF9QN2Y.search.serialssolutions.com?ctx_ver=Z39.88-2004&ctx_enc=info:ofi/enc:UTF-8&rfr_id=info:sid/ProQ%3Aasfaaquaticpollution&rft_val_fmt=info:ofi/fmt:kev:mtx:journal&rft.genre=article&rft.jtitle=Ecology+and+Society&rft.atitle=Evaluation+of+bayesian+networks+as+a+tool+for+participatory+water+resources+management%3A+application+to+the+upper+guadiana+basin+in+spain&rft.au=Zorrilla%2C+P%3BCarmona%2C+G%3BDe+la+Hera%2C+A%3BVarela-Ortega%2C+C%3BMartinez-Santos%2C+P%3BBromley%2C+J%3BHenriksen%2C+HJ&rft.aulast=Zorrilla&rft.aufirst=P&rft.date=2010-09-01&rft.volume=15&rft.issue=3&rft.spage=&rft.isbn=&rft.btitle=&rft.title=Ecology+and+Society&rft.issn=17083087&rft_id=info:doi/},
	abstract = {Stakeholder participation is becoming increasingly important in water resources management. In participatory processes, stakeholders contribute by putting forward their own perspective, and they benefit by enhancing their understanding of the factors involved in decision making. A diversity of modeling tools can be used to facilitate participatory processes. Bayesian networks are well suited to this task for a variety of reasons, including their ability to structure discussions and visual appeal. This research focuses on developing and testing a set of evaluation criteria for public participation. The advantages and limitations of these criteria are discussed in the light of a specific participatory modeling initiative. Modeling work was conducted in the Upper Guadiana Basin in central Spain, where uncontrolled groundwater extraction is responsible for wetland degradation and conflicts between farmers, water authorities, and environmentalists. Finding adequate solutions to the problem is urgent because the implementation of the EU Water Framework Directive requires all aquatic ecosystems to be in a "good ecological state" within a relatively short time frame. Stakeholder evaluation highlights the potential of Bayesian networks to support public participation processes.},
	language = {English},
	number = {3},
	journal = {Ecology and Society},
	author = {Zorrilla, P. and Carmona, G. and De la Hera, A. and Varela-Ortega, C. and Martinez-Santos, P. and Bromley, J. and Henriksen, H. J.},
	month = sep,
	year = {2010},
	keywords = {Evaluation, Spain, Ecology, Decision making, Water management, Water resources, Ecosystems, Groundwater, Wetlands, Aquatic ecosystems, Disputes, Ground water, Basins, Water resources management, ASFA 3: Aquatic Pollution \& Environmental Quality, D 04060:Management and Conservation, Ecology Abstracts, Q5 08503:Characteristics, behavior and fate, Water Resources Abstracts, M3 1010:Issues in Sustainable Development, Meteorological \& Geoastrophysical Abstracts, Sustainability Science Abstracts, AQ 00001:Water Resources and Supplies, Aqualine Abstracts, Model Studies, Bayesian analysis, conflicts, Degradation, environmentalism, M2 556.18:Water Management (556.18), Networks, Public Participation, Species diversity, stakeholders, SW 0840:Groundwater, Water authorities},
}

@article{sandker_exploring_2009,
	title = {Exploring the effectiveness of integrated conservation and development interventions in a {Central} {African} forest landscape},
	volume = {18},
	url = {http://pitt.idm.oclc.org/login?url=https://search.proquest.com/docview/222294962?accountid=14709 http://RT4RF9QN2Y.search.serialssolutions.com?ctx_ver=Z39.88-2004&ctx_enc=info:ofi/enc:UTF-8&rfr_id=info:sid/ProQ%3Aasfaaquaticpollution&rft_val_fmt=info:ofi/fmt:kev:mtx:journal&rft.genre=article&rft.jtitle=Biodiversity+%26+Conservation&rft.atitle=Exploring+the+effectiveness+of+integrated+conservation+and+development+interventions+in+a+Central+African+forest+landscape&rft.au=Sandker%2C+Marieke%3BCampbell%2C+Bruce+M%3BNzooh%2C+Zacharie%3BSunderland%2C+Terry%3BAmougou%2C+Victor%3BDefo%2C+Louis%3BSayer%2C+Jeffrey&rft.aulast=Sandker&rft.aufirst=Marieke&rft.date=2009-10-01&rft.volume=18&rft.issue=11&rft.spage=2875&rft.isbn=&rft.btitle=&rft.title=Biodiversity+%26+Conservation&rft.issn=09603115&rft_id=info:doi/10.1007%2Fs10531-009-9613-7},
	abstract = {Integrated conservation and development projects (ICDPs) have had limited success in addressing the often conflicting objectives of conservation and development. We developed a model with local participants to explore the trade-offs between conservation and development in southeastern Cameroon, where illegal hunting is regarded as the greatest challenge to conservation. We simulated the effects of different ICDP strategies by varying the degree of focus on antipoaching activities, anticorruption measures and direct development investments, and by varying the overall budget for such activities. Our outcome variables were numbers of selected wildlife species and household incomes. The model outcomes from the different scenarios were used to stimulate debate among stakeholders. Contributing to poverty alleviation while maintaining current animal population sizes will be extremely difficult and will require long-term external financial support. Devoting greater attention to improving local environmental governance emerged as the highest priority for this investment. We used the model outputs to inform some of the major policy makers in the region. Participatory modeling is a valuable means of capturing the complexities of achieving conservation at landscape scales and of stimulating innovative solutions to entrenched problems. [PUBLICATION ABSTRACT]},
	language = {English},
	number = {11},
	journal = {Biodiversity \& Conservation},
	author = {Sandker, Marieke and Campbell, Bruce M. and Nzooh, Zacharie and Sunderland, Terry and Amougou, Victor and Defo, Louis and Sayer, Jeffrey},
	month = oct,
	year = {2009},
	note = {Place: Dordrecht
Publisher: Springer Science \& Business Media},
	keywords = {Sustainable development, Conservation, Forests, Biology},
	pages = {2875--2892},
}

@article{videira_scoping_2009,
	title = {Scoping river basin management issues with participatory modelling: {The} {Baixo} {Guadiana} experience},
	volume = {68},
	url = {http://pitt.idm.oclc.org/login?url=https://search.proquest.com/docview/20389560?accountid=14709 http://RT4RF9QN2Y.search.serialssolutions.com?ctx_ver=Z39.88-2004&ctx_enc=info:ofi/enc:UTF-8&rfr_id=info:sid/ProQ%3Aasfaocean&rft_val_fmt=info:ofi/fmt:kev:mtx:journal&rft.genre=article&rft.jtitle=Ecological+Economics&rft.atitle=Scoping+river+basin+management+issues+with+participatory+modelling%3A+The+Baixo+Guadiana+experience&rft.au=Videira%2C+Nuno%3BAntunes%2C+Paula%3BSantos%2C+Rui&rft.aulast=Videira&rft.aufirst=Nuno&rft.date=2009-02-01&rft.volume=68&rft.issue=4&rft.spage=965&rft.isbn=&rft.btitle=&rft.title=Ecological+Economics&rft.issn=09218009&rft_id=info:doi/10.1016%2Fj.ecolecon.2008.11.008},
	abstract = {This paper details the design and implementation of a participatory modelling process in the Baixo Guadiana River Basin, in Portugal. A group of stakeholders was involved in a causal mapping exercise that lead to the development of a shared view of the problems, pressures and impacts characterizing the river basin. A simulation model was also developed to support experimentation with alternative management scenarios for the area. The paper looks critically at the evaluation of the participatory modelling outcomes, both at the individual and group levels, discussing the role of this approach in supporting the scoping stages of river basin planning and management processes. On the downside, the unstable group composition seems to have hindered the chances of producing a higher impact in the functioning of the group, and subsequently the capacity to sustain the level of collaboration required to achieve the strategic river basin objectives established during the participatory modelling process. The paper discusses the issue of group stability as well as some options to overcome the limitations of unstable participant groups. The strengths of participatory modelling, as underlined by the Baixo Guadiana experience, include the flexibility to adapt the method to different contexts and participatory designs and the capacity to structure the active involvement of stakeholders, providing an open and shared language for collaborative policy design, fostering learning and knowledge integration.},
	language = {English},
	number = {4},
	journal = {Ecological Economics},
	author = {Videira, Nuno and Antunes, Paula and Santos, Rui},
	month = feb,
	year = {2009},
	note = {Publisher: Elsevier Science, P.O. Box 211 Amsterdam 1000 AE Netherlands, [mailto:nlinfo-f@elsevier.nl]},
	keywords = {Learning, Economics, Portugal, Simulation, Mathematical models, Water resources, Mapping, Models, Policies, D 04060:Management and Conservation, Ecology Abstracts, ASFA 2: Ocean Technology Policy \& Non-Living Resources, M3 1010:Issues in Sustainable Development, Sustainability Science Abstracts, Q2 09122:Legislation, Freshwater, stakeholders, Baixo Guadiana, Europe, Iberian Peninsula, Guadiana R. basin, Integration, Language, Participatory modelling, Physical training, River basin management, River basin planning and management, River basins, System dynamics, Water Framework Directive},
	pages = {965--978},
}

@article{beall_participatory_2008,
	title = {Participatory modeling of endangered wildlife systems: {Simulating} the sage-grouse and land use in {Central} {Washington}},
	volume = {68},
	url = {http://pitt.idm.oclc.org/login?url=https://search.proquest.com/docview/19495075?accountid=14709 http://RT4RF9QN2Y.search.serialssolutions.com?ctx_ver=Z39.88-2004&ctx_enc=info:ofi/enc:UTF-8&rfr_id=info:sid/ProQ%3Aasfaaquaticpollution&rft_val_fmt=info:ofi/fmt:kev:mtx:journal&rft.genre=article&rft.jtitle=Ecological+Economics&rft.atitle=Participatory+modeling+of+endangered+wildlife+systems%3A+Simulating+the+sage-grouse+and+land+use+in+Central+Washington&rft.au=Beall%2C+Allyson%3BZeoli%2C+Len&rft.aulast=Beall&rft.aufirst=Allyson&rft.date=2008-12-01&rft.volume=68&rft.issue=1-2&rft.spage=24&rft.isbn=&rft.btitle=&rft.title=Ecological+Economics&rft.issn=09218009&rft_id=info:doi/10.1016%2Fj.ecolecon.2008.08.019},
	abstract = {The Greater sage-grouse (Centrocercus urophasianus) occupies the sage brush habitats of Western North America. Large population declines in the last several decades have made it a candidate for possible listing under the Endangered Species Act. Listing was recently avoided in part because local working groups are developing long-range management plans in conjunction with federal and local agencies. The Foster Creek Conservation District, a working group in Douglas County, Washington, saw the potential for system dynamics to synthesize known sage-grouse dynamics and local land use patterns to support development of their Habitat Conservation Plan and subsequent land management decisions. This case study highlights the integration of science, local knowledge and social concerns into a participatory process that uses system dynamics as a forum for the exploration of the impacts of land management decisions upon the sage-grouse population and the landowners of Douglas County, Washington.},
	language = {English},
	number = {1-2},
