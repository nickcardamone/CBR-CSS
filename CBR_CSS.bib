	journal = {International Journal of Integrated Care (IJIC)},
	author = {McKelvie, Douglas and Arnold, Steve and Dafter, Terry},
	year = {2015},
	keywords = {system dynamics, COMPUTER simulation, HEALTH planning, HOSPITAL admission \& discharge, integration, JOINT Commission on Accreditation of Healthcare Organizations, modelling, PRIMARY care, simulation, STOCKPORT (England)},
