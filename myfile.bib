@Article{4005,
author="Vieira Pak, Manuela
and Castillo Brieva, Daniel",
title="Designing and implementing a Role-Playing Game: A tool to explain factors, decision making and landscape transformation",
journal="Environmental Modelling {\&} Software",
year="2010",
