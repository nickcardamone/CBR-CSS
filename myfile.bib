}

@Article{2209,
author="Baird, Julia
and Plummer, Ryan
and Haug, Constanze
and Huitema, Dave",
title="Learning effects of interactive decision-making processes for climate change adaptation",
journal="Global Environmental Change",
year="July 2014 2016",
month="Dec",
day="22",
publisher="Elsevier B.V., The Boulevard Kidlington Oxford OX5 1GB United Kingdom",
volume="27",
pages="51-63",
keywords="ASFA 3: Aquatic Pollution {\&} Environmental Quality Environment Abstracts Meteorological {\&} Geoastrophysical Abstracts Sustainability Science Abstracts Learning Adaptive co-management Climate change adaptation Adaptive capacity Environmental governance Adaptations ANW, Canada Methodology Adaptability Environmental changes Climatic changes Environmental factors Biological surveys Climate change Canada Fellowships Typology Social behaviour Stakeholders Q5 08504:Effects on organisms ENA 20:Weather Modification {\&} Geophysical Change M2 551.583:Variations (551.583) M3 1010:Issues in Sustainable Development",
abstract="Learning is gaining attention in relation to governance processes for contemporary environmental challenges; however, scholarship at the nexus of learning and environmental governance lacks clarity and understanding about how to define and measure learning, and the linkages between learning, social interactions, and environment. In response, this study aimed to advance and operationalize a typology of learning in an environmental governance context, and examined if a participatory decision-making process (adaptive co-management) for climate change adaptation fostered learning. Three types of learning were identified: cognitive learning, related to the acquisition of new or the structuring of existing knowledge; normative learning, which concerns a shift in viewpoints, values or paradigms, and relational learning, referring to an improved understanding of others' mindsets, enhanced trust and ability to cooperate. A robust mixed methods approach with a focus on quantitative measures including concept map analysis, social network analysis, and self-reflective questions, was designed to gauge indicators for each learning type. A participatory decision-making process for climate change adaptation was initiated with stakeholders in the Niagara region, Canada. A pseudo-control group was used to minimize external contextual influences on results. Clear empirical evidence of cognitive and relational learning was gained; however, the results from normative learning measures were inconclusive. The learning typology and measurement method operationalized in this research advances previous treatments of learning in relation to participatory decision-making processes, and supports adaptive co-management as a governance strategy that fosters learning and adaptive capacity.",
note="Date revised - 2014-11-01 SubjectsTermNotLitGenreText - Biological surveys; Typology; Adaptations; Social behaviour; Climatic changes; Climate change; Environmental factors; Fellowships; Methodology; Stakeholders; Adaptability; Environmental changes; Canada; ANW, Canada",
url="http://pitt.idm.oclc.org/login?url=https://search.proquest.com/docview/1627977006?accountid=14709 http://RT4RF9QN2Y.search.serialssolutions.com?ctx_ver=Z39.88-2004&ctx_enc=info:ofi/enc:UTF-8&rfr_id=info:sid/ProQ%3Aasfaaquaticpollution&rft_val_fmt=info:ofi/fmt:kev:mtx:journal&rft.genre=article&rft.jtitle=Global+Environmental+Change&rft.atitle=Learning+effects+of+interactive+decision-making+processes+for+climate+change+adaptation&rft.au=Baird%2C+Julia%3BPlummer%2C+Ryan%3BHaug%2C+Constanze%3BHuitema%2C+Dave&rft.aulast=Baird&rft.aufirst=Julia&rft.date=2014-07-01&rft.volume=27&rft.issue=&rft.spage=51&rft.isbn=&rft.btitle=&rft.title=Global+Environmental+Change&rft.issn=09593780&rft_id=info:doi/10.1016%2Fj.gloenvcha.2014.04.019",
language="English"
}

@Article{2247,
author="Haase, Dagmar",
title="Participatory modelling of vulnerability and adaptive capacity in flood risk management",
journal="Natural Hazards",
year="May 2013 2014",
month="Aug",
day="22",
publisher="Springer Nature B.V.",
address="Dordrecht",
volume="67",
number="1",
pages="77-97",
keywords="Earth Sciences",
abstract="Issue Title: SOCIAL RESILIENCE IN HAZARD RESEARCH AND PLANNING Being part of the EU-project NeWater on adaptive water resources management, the Ukrainian Tisa river basin is presented as an example for a participatory study dealing with flood risk, vulnerability and adaptive capacity. The Tisa valley is regularly and increasingly faced with hazardous floods at very limited local budgets and high poverty rates. In order to make flood risk management more resilient and better adapted to climate change, scientists and stakeholders applied a set of qualitative and quantitative modelling approaches to characterise prevailing flood risk management, to discover respective vulnerabilities and to identify barriers and options of adaptive capacity. The former were found in the defensive mentality paradigm and the inert and hierarchical structure of present institutions, the latter in, firstly, an enormous potential to link the knowledge of different stakeholders in the region, secondly, a better integration of the individual flood preparedness of households and thirdly, the active involvement of the Church as institution in local flood risk management.[PUBLICATION ABSTRACT]",
note="Copyright - Springer Science+Business Media Dordrecht 2013",
url="http://pitt.idm.oclc.org/login?url=https://search.proquest.com/docview/1364602262?accountid=14709 http://RT4RF9QN2Y.search.serialssolutions.com?ctx_ver=Z39.88-2004&ctx_enc=info:ofi/enc:UTF-8&rfr_id=info:sid/ProQ%3Aasfaocean&rft_val_fmt=info:ofi/fmt:kev:mtx:journal&rft.genre=article&rft.jtitle=Natural+Hazards&rft.atitle=Participatory+modelling+of+vulnerability+and+adaptive+capacity+in+flood+risk+management&rft.au=Haase%2C+Dagmar&rft.aulast=Haase&rft.aufirst=Dagmar&rft.date=2013-05-01&rft.volume=67&rft.issue=1&rft.spage=77&rft.isbn=&rft.btitle=&rft.title=Natural+Hazards&rft.issn=0921030X&rft_id=info:doi/10.1007%2Fs11069-010-9704-5",
language="English"
}

